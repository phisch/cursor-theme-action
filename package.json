{
  "name": "@phisch/figma-cursor-theme-action",
  "description": "GitHub Action that generates a cursor theme from a Figma file.",
  "scripts": {
    "compile": "tsc",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build build/main.js --source-map --license licenses.txt",
    "build": "npm run compile && npm run lint && npm run package",
    "start": "ts-node src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phisch/figma-cursor-theme-action.git"
  },
  "keywords": [
    "github-action",
    "figma",
    "cursor-theme",
    "xcursor"
  ],
  "author": "Philipp Schaffrath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phisch/figma-cursor-theme-action/issues"
  },
  "homepage": "https://github.com/phisch/figma-cursor-theme-action#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@svgdotjs/svg.js": "^3.1.2",
    "@types/sharp": "^0.30.2",
    "axios": "^0.27.2",
    "figma-api": "^1.10.1",
    "sharp": "^0.30.4",
    "svgdom": "^0.1.10",
    "toml": "^3.0.0",
    "xcursor": "^1.1.7"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/parser": "^5.22.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.14.0",
    "eslint-plugin-github": "^4.3.6",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
